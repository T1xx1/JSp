{
	"include": ["./src"],
	"exclude": ["./node_modules"],
	"compilerOptions": {
		"skipLibCheck": true,
		/* env */
		"module": "commonjs",
		"moduleResolution": "node",
		/* type checker */
		"strict": true,
		/* compiler */
		"target": "ES2024",
		"sourceMap": true,
		"lib": ["ES2024"],
		"rootDir": "./src",
		"outDir": "./out"
	}
}
